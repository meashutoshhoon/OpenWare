<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/base"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background="?attr/colorSurface"
    android:orientation="vertical"
    android:paddingStart="8dp"
    android:paddingTop="8dp"
    android:paddingBottom="8dp"
    tools:ignore="RtlSymmetry">


    <LinearLayout
        android:id="@+id/linear_editors"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linear2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_weight="1"
                android:fontFamily="@font/en_light"
                android:padding="8dp"
                android:text="Editor's Choice"
                android:textAppearance="?attr/textAppearanceHeadline6" />

            <Button
                android:id="@+id/editor_seemore"
                style="@style/Widget.Material3Expressive.Button.TextButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:fontFamily="@font/en_light"
                android:padding="8dp"
                android:text="SEE MORE"
                android:textSize="13.5sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_shimmer1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linear_box6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="8dp"
                android:gravity="center"
                android:orientation="vertical">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:orientation="horizontal">

                    <include layout="@layout/project_banner_shrimmer" />

                </com.facebook.shimmer.ShimmerFrameLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_box7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    android:orientation="horizontal">

                    <include layout="@layout/project_banner_shrimmer" />

                </com.facebook.shimmer.ShimmerFrameLayout>
            </LinearLayout>
        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerview_editors"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="3dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_latest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linear3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_weight="1"
                android:fontFamily="@font/en_light"
                android:padding="8dp"
                android:text="New &amp; Updated"
                android:textAppearance="?attr/textAppearanceHeadline6" />

            <Button
                android:id="@+id/textview7"
                style="@style/Widget.Material3Expressive.Button.TextButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:fontFamily="@font/en_light"
                android:padding="8dp"
                android:text="SEE MORE"
                android:textSize="13.5sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_shimmer2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.facebook.shimmer.ShimmerFrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp" />

                </LinearLayout>


            </com.facebook.shimmer.ShimmerFrameLayout>

            <LinearLayout
                android:id="@+id/linear_box1"
                android:layout_width="110dp"
                android:layout_height="140dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="7dp"
                android:layout_marginBottom="5dp"
                android:background="#FFFFFF"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="4dp">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic1"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/tt1"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="4dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_box2"
                android:layout_width="110dp"
                android:layout_height="140dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="7dp"
                android:layout_marginBottom="5dp"
                android:background="#FFFFFF"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="4dp">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic2"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/tt2"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginBottom="4dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_box3"
                android:layout_width="110dp"
                android:layout_height="140dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="7dp"
                android:layout_marginBottom="5dp"
                android:background="#FFFFFF"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="5dp">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic3"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/tt3"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginBottom="4dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_box4"
                android:layout_width="110dp"
                android:layout_height="140dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="7dp"
                android:layout_marginBottom="5dp"
                android:background="#FFFFFF"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="5dp">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic4"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/tt4"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginBottom="4dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_box5"
                android:layout_width="110dp"
                android:layout_height="140dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="7dp"
                android:layout_marginBottom="5dp"
                android:background="#FFFFFF"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="5dp">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/ic5"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/tt5"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginBottom="4dp"
                    android:background="#E0E0E0"
                    android:orientation="horizontal"
                    android:padding="8dp" />
            </LinearLayout>
        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerview_latest"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="3dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_most_liked"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linear5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="4dp">

            <TextView
                android:id="@+id/textview10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_weight="1"
                android:fontFamily="@font/en_light"
                android:padding="8dp"
                android:text="Most Liked"
                android:textAppearance="?attr/textAppearanceHeadline6" />

            <Button
                android:id="@+id/textview11"
                style="@style/Widget.Material3Expressive.Button.TextButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:fontFamily="@font/en_light"
                android:padding="8dp"
                android:text="SEE MORE"
                android:textSize="13.5sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear_shimmer4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="4dp">

            <com.facebook.shimmer.ShimmerFrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:layout_marginEnd="12dp" />

                    <include
                        layout="@layout/project_shrimmer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp" />

                </LinearLayout>


            </com.facebook.shimmer.ShimmerFrameLayout>
        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerview_liked"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>


</LinearLayout>